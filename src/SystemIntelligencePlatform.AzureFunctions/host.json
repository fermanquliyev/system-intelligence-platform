{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Trace"
    }
  },
  "extensions": {
    "serviceBus": {
      "prefetchCount": 10,
      "messageHandlerOptions": {
        "maxConcurrentCalls": 16,
        "autoComplete": true,
        "maxAutoLockRenewalDuration": "00:05:00"
      },
      "maxDeliveryCount": 10,
      "retryOptions": {
        "mode": "Exponential",
        "maxRetries": 5,
        "delay": "00:00:02",
        "maxDelay": "00:01:00"
      }
    }
  }
}
